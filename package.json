{
  "name": "react-intl-redux",
  "version": "0.7.0",
  "description": "Redux binding for React Intl",
  "main": "lib/index",
  "side-effects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib",
    "clean": "del lib coverage",
    "lint": "eslint --ignore-path .gitignore --ext js,md .",
    "prepublish": "run-s clean build",
    "test": "run-p test:* lint",
    "test:ava": "ava",
    "coverage": "nyc ava",
    "precommit": "lint-staged"
  },
  "peerDependencies": {
    "react-intl": "^2.2.2",
    "react-redux": "^5.0.1"
  },
  "dependencies": {
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.5",
    "del-cli": "^1.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-concise": "^0.25.0",
    "eslint-config-concise-ava": "^0.23.0",
    "eslint-config-concise-esnext": "^0.24.1",
    "eslint-config-concise-react": "^0.25.0",
    "husky": "^0.14.3",
    "immutable": "^3.8.2",
    "jsdom": "^11.10.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^7.1.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^11.8.0",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.3.2",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0"
  },
  "repository": "ratson/react-intl-redux",
  "license": "MIT",
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "keywords": [
    "react",
    "reactjs",
    "react-intl",
    "react-redux",
    "intl",
    "i18n",
    "redux"
  ],
  "ava": {
    "require": [
      "babel-register",
      "raf/polyfill",
      "jsdom-global/register",
      "./test/helpers/enzyme-config"
    ],
    "babel": "inherit"
  },
  "eslintConfig": {
    "extends": [
      "concise",
      "concise-esnext",
      "concise-ava",
      "concise-react"
    ],
    "rules": {
      "comma-dangle": "off"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "nyc": {
    "reporter": [
      "html",
      "lcov",
      "text"
    ]
  }
}
